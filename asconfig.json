{
    "targets": {
        "release": {
            "outFile": "build/ReentrantToken.wasm",
            "textFile": "build/ReentrantToken.wat",
            "sourceMap": false,
            "optimizeLevel": 3,
            "shrinkLevel": 2,
            "converge": true,
            "noAssert": true,
            "disable": [
                "mutable-globals",
                "sign-extension",
                "nontrapping-f2i",
                "bulk-memory"
            ],
            "runtime": "stub",
            "memoryBase": 0,
            "initialMemory": 1,
            "bindings": "esm",
            "exportStart": "start",
            "use": [
                "abort=src/index/abort"
            ]
        }
    },
    "options": {
        "transform": "@btc-vision/opnet-transform"
    }
}
